{
  "name": "producdev",
  "displayName": "ProducDev",
  "description": "A developer productivity tool",
  "version": "0.0.1",
  "publisher": "fsa-producdev",
  "repository": {
    "type": "git",
    "url": "https://github.com/2011-Team-Virgo/ProducDev-extension.git"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "producdev-sidebar-view",
          "title": "ProducDev",
          "icon": "media/root-folder.svg"
        }
      ]
    },
    "views": {
      "producdev-sidebar-view": [
        {
          "type": "webview",
          "id": "producdev-sidebar",
          "name": "ProducDev",
          "icon": "media/root-folder.svg",
          "contextualTitle": "ProducDev"
        }
      ]
    },
    "commands": [
      {
        "command": "producdev.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "producdev.refresh",
        "category": "ProducDev",
        "title": "Refresh"
      },
      {
        "command": "producdev.authenticate",
        "category": "ProducDev",
        "title": "Authenticate"
      },
      {
        "command": "extension.getGitHubUser",
        "title": "Get GitHub User",
        "category": "GitHub Authentication Sample"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "start": "nodemon src/server/index.js",
    "build": "npm run lint && tsc"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/polka": "^0.5.1",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.0",
    "svelte-check": "^1.1.23",
    "svelte-preprocess": "^4.6.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "@octokit/rest": "^18.0.0",
    "axios": "^0.21.1",
    "dateformat": "^4.5.1",
    "express": "^4.17.1",
    "firebase": "^8.2.5",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  }
}
